# GitHub API (optional - increases rate limit from 60 to 5000 requests/hour)
GITHUB_TOKEN=

# OpenAI API (required for AI-powered README analysis)
OPENAI_API_KEY=

# Upstash Redis (auto-configured when you add Upstash via Vercel Storage)
# These are automatically set by Vercel when you connect Upstash Redis
KV_REST_API_URL=
KV_REST_API_TOKEN=

# Alternative Upstash env var names (also supported)
# UPSTASH_REDIS_REST_URL=
# UPSTASH_REDIS_REST_TOKEN=
